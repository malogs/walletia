{
  "expo": {
    "name": "walletia",
    "slug": "walletia",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "scheme": "walletia",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.walletia",
      "infoPlist": {
        "LSApplicationQueriesSchemes": [
          "tel",
          "telprompt",
          "tel",
          "telprompt"
        ],
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "bundler": "metro",
      "package": "com.walletia",
      "permissions": [
        "CALL_PHONE",
        "READ_CONTACTS"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-asset",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "./assets/splash-icon.png"
        }
      ],
      "react-native-edge-to-edge"
    ],
    "extra": {
      "eas": {
        "projectId": "5f6783ca-f130-4db6-8e36-0234c2d3523d"
      }
    }
  }
}
